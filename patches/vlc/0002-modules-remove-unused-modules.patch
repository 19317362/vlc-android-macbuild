+if !MODULE_LITE
 libmkv_plugin_la_SOURCES = \
 	demux/mkv/util.hpp demux/mkv/util.cpp \
 	demux/mkv/virtual_segment.hpp demux/mkv/virtual_segment.cpp \
@@ -180,6 +183,7 @@ libmkv_plugin_la_LIBADD += -lz
 endif
 demux_LTLIBRARIES += $(LTLIBmkv)
