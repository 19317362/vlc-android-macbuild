From: bbcallen <bbcallen@gmail.com>
Date: Mon, 16 Apr 2012 23:20:57 +0800
Subject: [PATCH 3/5] lite-mode: configure.sh: disable modules

---
 configure.sh |    8 ++++----
 1 个文件被修改，插入 4 行(+)，删除 4 行(-)

diff --git a/configure.sh b/configure.sh
index cd280a7..546a1aa 100755
--- a/configure.sh
+++ b/configure.sh
@@ -46,7 +46,7 @@ STRIP="${CROSS_COMPILE}strip" \
 RANLIB="${CROSS_COMPILE}ranlib" \
 AR="${CROSS_COMPILE}ar" \
 sh $VLC_SOURCEDIR/configure --host=arm-linux-androideabi --build=x86_64-unknown-linux $EXTRA_PARAMS \
-                --enable-live555 --enable-realrtsp \
+                --disable-live555 --disable-realrtsp \
                 --enable-avformat \
                 --enable-swscale \
                 --enable-avcodec \
@@ -54,8 +54,9 @@ sh $VLC_SOURCEDIR/configure --host=arm-linux-androideabi --build=x86_64-unknown-
                 --enable-audiotrack \
                 --enable-android-surface \
                 --enable-debug \
-                --enable-mkv \
-                --enable-taglib \
+                --disable-mkv \
+                --disable-taglib \
+                --disable-freetype \
                 --enable-iomx \
                 --disable-vlc --disable-shared \
                 --disable-vlm \
@@ -95,7 +96,6 @@ sh $VLC_SOURCEDIR/configure --host=arm-linux-androideabi --build=x86_64-unknown-
                 --disable-svg \
                 --disable-sqlite \
                 --disable-udev \
-                --disable-libxml2 \
                 --disable-caca \
                 --disable-glx \
                 --disable-egl \
