diff --git a/configure.sh b/configure.sh
index 2c5cdae..c6d9f25 100755
--- a/configure.sh
+++ b/configure.sh
@@ -57,9 +57,11 @@ sh $VLC_SOURCEDIR/configure --host=$TARGET_TUPLE --build=x86_64-unknown-linux $E
                 --enable-opensles \
                 --enable-audiotrack \
                 --enable-android-surface \
-                --enable-mkv \
-                --enable-taglib \
+                --disable-mkv \
+                --disable-taglib \
                 --enable-dvbpsi \
+                --enable-debug \
+                --disable-freetype \
                 --enable-iomx \
                 --disable-vlc --disable-shared \
                 --disable-vlm \
@@ -95,7 +97,7 @@ sh $VLC_SOURCEDIR/configure --host=$TARGET_TUPLE --build=x86_64-unknown-linux $E
                 --disable-skins2 \
                 --disable-mtp \
                 --disable-notify \
-                --enable-libass \
+                --disable-libass \
                 --disable-svg \
                 --disable-sqlite \
